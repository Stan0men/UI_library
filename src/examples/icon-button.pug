.icon-button-wrapper
    h2 Icons Button
    .block-container
        h3 icon button
        <i class="fas fa-align-justify"></i>
        <i class="fas fa-arrow-left"></i>
        <i class="fas fa-arrow-right"></i>
        <i class="fas fa-check"></i>
        <i class="fas fa-times"></i>
        <i class="fas fa-expand"></i>
        <i class="fas fa-compress"></i>
        <i class="fas fa-ellipsis-v"></i>
        <i class="fas fa-redo"></i>
    .block-container.styled
        h3 styled
        <i class="fas fa-heart heart_black"></i>
        <i class="fas fa-heart heart_red" ></i>
        <i class="fas fa-heart heart_grey" ></i>
    .block-container.segmented
        h3 segmented
        <i class="fas fa-male"></i>
        <i class="fas fa-bicycle"></i>
        <i class="fas fa-bus"></i>
        <i class="fas fa-car"></i>
    h2 Floating Action Buttons
    .block-container.regular
        h3 Regular
        <i class="fas fa-arrow-right big-icon white-color red-background"></i>
        <i class="fas fa-pencil-alt big-icon white-color light-blue-background"></i>
        <i class="fas fa-plus icon big-icon icon-shadow_1"></i>
    .block-container.mini
        h3 Mini
        <i class="fas fa-check small-icon green-background white-color"></i>
        <i class="fas fa-undo small-icon purple-background white-color"></i>
        <i class="fas fa-times small-icon icon-shadow_1"></i>